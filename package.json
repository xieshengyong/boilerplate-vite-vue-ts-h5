{
  "name": "boilerplate-vite-vue-ts-h5",
  "private": true,
  "version": "0.0.1",
  "description": "基于vite-vue-ts的常规H5模板",
  "author": "xieshengyong",
  "repository": {
    "type": "git",
    "url": "https://github.com/xieshengyong/boilerplate-vite-vue-ts-h5"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "postbuild": "npm run movehtml",
    "tx": "vue-tsc && vite build --mode tx",
    "posttx": "npm run movehtml",
    "preview": "vite preview",
    "movehtml": "move-file dist/ossweb-img/index.html dist/index.html",
    "copyfilesqq": "copy-file dist/index.html dist/index_qq.html",
    "replaceqq": "cd dist/ && replace 'index.js' 'index_qq.js' -r --include=index_qq.html",
    "trashvideo": "trash dist/ossweb-img/*.mp4",
    "zip": "cd dist && bestzip dist.zip ossweb-img/ index.html",
    "sftp": "node sftp.js",
    "qshell": "qshell user cu td && qshell qupload -c 30 .qshell.json",
    "ssh": "ssh -D 0.0.0.0:20000 treedom@8.135.58.157 -N",
    "ff": "ffmpeg -i ./tempAss/首页入场.mp4 -f mpegts -codec:v mpeg1video -s 1500x3200 -b:v 1500k -r 25 -bf 0 -codec:a mp2 -ar 44100 -ac 1 -b:a 128k ./src/assets/media/index_in.ttf",
    "audio": "audiosprite -f howler2 -b 128 -o ./src/assets/media/mc -e mp3 ./src/assets/media/audio/mc/*.mp3",
    "audiozip": "audiosprite -f howler2 -b 64 -o ./src/assets/media/bgm2 -e mp3 ./src/assets/media/bgm.mp3"
  },
  "dependencies": {
    "clipboard": "^2.0.11",
    "gsap": "^3.12.4",
    "howler": "^2.2.4",
    "html2canvas": "^1.4.1",
    "lottie-web": "^5.12.2",
    "qrcode": "^1.5.3",
    "senyou-tool": "^0.0.5",
    "vue": "^3.3.8"
  },
  "devDependencies": {
    "@types/howler": "^2.2.11",
    "@types/node": "^20.10.0",
    "@types/zepto": "^1.0.36",
    "@vitejs/plugin-legacy": "^5.2.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "bestzip": "^2.2.1",
    "copy-file-util": "^1.1.3",
    "image-size": "^1.0.2",
    "less": "^4.2.0",
    "move-file-cli": "^3.0.0",
    "postcss-pxtorem": "^6.0.0",
    "replace": "^1.2.2",
    "ssh2-sftp-client": "^9.1.0",
    "trash-cli": "^5.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-html": "^3.2.2",
    "vue-tsc": "^1.8.22"
  }
}